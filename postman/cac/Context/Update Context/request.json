{
  "method": "PUT",
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer {{token}}",
      "type": "text"
    },
    {
      "key": "Content-Type",
      "value": "application/json",
      "type": "text"
    }
  ],
  "body": {
    "mode": "raw",
    "options": {
      "raw": {
        "language": "json"
      }
    },
    "raw_json_formatted": {
      "override": {
        "key1": "value3"
      },
      "context": {
        "==": [
          {
            "var": "clientId"
          },
          "piyaz"
        ]
      }
    }
  },
  "url": {
    "raw": "{{host}}/context",
    "host": [
      "{{host}}"
    ],
    "path": [
      "context"
    ]
  }
}
