{
  "method": "PUT",
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer {{token}}",
      "type": "text"
    },
    {
      "key": "Content-Type",
      "value": "application/json",
      "type": "text"
    }
  ],
  "body": {
    "mode": "raw",
    "options": {
      "raw": {
        "language": "json"
      }
    },
    "raw_json_formatted": {
      "dimension": "clientId",
      "priority": 100,
      "schema": {
        "type": "string",
        "pattern": "^[a-z0-9].*$"
      }
    }
  },
  "url": {
    "raw": "{{host}}/dimension",
    "host": [
      "{{host}}"
    ],
    "path": [
      "dimension"
    ]
  }
}
